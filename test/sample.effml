doctype 'html'
head {
  title 'Page Title'
  meta {
    charset 'UTF-8'
  }
  meta {
    viewport 'width=device-width, initial-scale=1.0'
  }
  link {
    rel 'stylesheet'
    href 'styles.css'
  }
  link {
    rel 'icon'
    href 'favicon.ico'
  }
}
body {
  h1 {
    'Hello, World!'
  }
  p {
    'This is an example HTML document.'
    'Another paragraph line.'
    div {
      'Some text inside a div.'
    }
  }
  img {
    src 'image.jpg'
    alt 'Sample Image'
  }
}
