doctype 'html'
html {
  head {
    meta {
      charset 'UTF-8'
    }
    meta {
      name 'viewport'
      content 'width=device-width, initial-scale=1.0'
    }
    link {
      rel 'stylesheet'
      href 'assets\\styles.css'
    }
    link {
      rel 'icon'
      href 'favicon.ico'
    }
    title {
      'Page\'s Title'
    }
  }
  body {
    h1 {
      'Hello, World!'
    }
    p {
      'It\'s awesome\n      EffML document.'
      div {
        'Some\tnasty\rpiece\fof\btext'
      }
      img {
        src 'image.jpg'
        alt 'Sample Image'
      }
    }
  }
}
